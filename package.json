  "scripts": {
    "start": "webpack serve --open --config webpack.config.js --mode development",
    "start-https": "HTTPS=true npm start",
    "startServer:test": "cd server && npm install && npm run start:test",
    "build": "webpack --mode production",
    "eject": "react-scripts eject",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "test": "concurrently \"npm run startServer:test\" \"start-server-and-test http://localhost:3001 && cypress open\"",
    "test:local": "concurrently \"npm run startServer:test\" \"npm run start\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "report": "webpack --mode production --profile --json > stats.json; webpack-bundle-analyzer ./stats.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
